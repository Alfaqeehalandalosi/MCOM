            border: none;
            cursor: pointer;
        }

        .btn:hover {
            background-color: var(--secondary-color);
        }
        
        /* Animation Effects */
        .fade-in-up {
            opacity: 0;
            transform: translateY(20px);
            transition: opacity 1s cubic-bezier(0.4, 0, 0.2, 1), transform 1s cubic-bezier(0.4, 0, 0.2, 1);
        }

        .fade-in-up.is-visible {
            opacity: 1;
            transform: translateY(0);
        }
        
