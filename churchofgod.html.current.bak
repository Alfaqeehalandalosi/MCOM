            max-width: 900px;
            margin: 0 auto;
        }

        .get-started-content ul {
            list-style-position: inside;
            text-align: left;
            display: inline-block;
            margin-top: 1em;
            margin-bottom: 1em;
        }

        .get-started-content ul li {
