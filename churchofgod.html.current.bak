        }

        .btn {
            display: inline-block;
            padding: 0.85em 1.5em;
            margin: 0.5em;
            background-color: var(--black);
            color: var(--white);
            font-family: var(--font-open-sans);
            text-transform: uppercase;
            letter-spacing: 0.25em;
            border-radius: 5px;
